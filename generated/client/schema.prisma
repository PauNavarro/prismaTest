}

model autonomy {
  id          Int          @id @default(autoincrement())
  name        String       @db.VarChar(100)
  countryFk   Int          @db.UnsignedMediumInt
  geoFk       Int?
  country     country      @relation(fields: [countryFk], references: [id], onDelete: Cascade, map: "autonomy_FK")
